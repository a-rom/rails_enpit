<p id="notice"><%= notice %></p>

<h1>Listing Books</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Date</th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><a href="/book/<%= book.isbn %>"><%= book.title %></a></td>
        <td><%= book.author %></td>
        <td><%= book.salesDate %></td>
      </tr>
    <% end %>
  </tbody>
</table>
